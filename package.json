{
  "name": "@the-homeless-god/chrome-bionic-reader",
  "version": "1.0.0",
  "description": "Tests for Chrome Bionic Reader extension",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --verbose",
    "test:badges": "jest-coverage-badges",
    "dev": "vite",
    "build": "vite build && node scripts/build.js",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "check-types": "tsc --noEmit",
    "validate": "npm run lint && npm run check-types && npm run test",
    "test:e2e": "jest src/tests/e2e --runInBand"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/register": "^7.23.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/chrome": "^0.0.260",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.24",
    "@types/puppeteer": "^7.0.4",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "fs-extra": "^11.3.0",
    "jest": "^29.7.0",
    "jest-coverage-badges": "^1.1.2",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.5",
    "puppeteer": "^24.2.1",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.4"
  },
  "dependencies": {
    "fp-ts": "^2.16.9"
  }
}
